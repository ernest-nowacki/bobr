{
  "name": "turbo-2",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "turbo": "2.6.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "bun": ">=1.3.2"
  },
  "packageManager": "bun@1.3.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
